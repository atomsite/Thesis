@article{vinkMetallicityDependenceWolfRayet2005,
  title = {On the Metallicity Dependence of {{Wolf-Rayet}} Winds},
  author = {Vink, Jorick S. and de Koter, A.},
  options = {useprefix=true},
  date = {2005-11},
