@article{dougherty_radio_2003,
  title = {Radio Emission Models of Colliding-Wind Binary Systems},
  author = {Dougherty, S. M. and Pittard, J. M. and Kasian, L. and Coker, R. F. and Williams, P. M. and Lloyd, H. M.},
  date = {2003-10},
  journaltitle = {Astronomy \& Astrophysics},
