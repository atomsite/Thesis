@article{seeEffectsStellarWinds2014,
  title = {The Effects of Stellar Winds on the Magnetospheres and Potential Habitability of Exoplanets},
  author = {See, V. and Jardine, M. and Vidotto, A. A. and Petit, P. and Marsden, S. C. and Jeffers, S. V. and do Nascimento, J. D.},
  date = {2014-10-01},
  journaltitle = {A\&A},
  volume = {570},
  pages = {A99},
  publisher = {{EDP Sciences}},
  issn = {0004-6361, 1432-0746},
  doi = {10.1051/0004-6361/201424323},
  url = {https://www.aanda.org/articles/aa/abs/2014/10/aa24323-14/aa24323-14.html},
  urldate = {2021-09-04},
  abstract = {\emph{Context. {$<$}i/{$>$}The principle definition of habitability for exoplanets is whether they can sustain liquid water on their surfaces, i.e. that they orbit within the habitable zone. However, the planet's magnetosphere should also be considered, since without it, an exoplanet's atmosphere may be eroded away by stellar winds.\emph{Aims. {$<$}i/{$>$}The aim of this paper is to investigate magnetospheric protection of a planet from the effects of stellar winds from solar-mass stars.\emph{Methods. {$<$}i/{$>$}We study hypothetical Earth-like exoplanets orbiting in the host star's habitable zone for a sample of 124 solar-mass stars. These are targets that have been observed by the Bcool Collaboration. Using two wind models, we calculate the magnetospheric extent of each exoplanet. These wind models are computationally inexpensive and allow the community to quickly estimate the magnetospheric size of magnetised Earth-analogues orbiting cool stars.\emph{Results. {$<$}i/{$>$}Most of the simulated planets in our sample can maintain a magnetosphere of \textasciitilde 5 Earth radii or larger. This suggests that magnetised Earth analogues in the habitable zones of solar analogues are able to protect their atmospheres and is in contrast to planets around young active M dwarfs. In general, we find that Earth-analogues around solar-type stars, of age 1.5 Gyr or older, can maintain at least a Paleoarchean Earth sized magnetosphere. Our results indicate that planets around 0.6\textendash 0.8 solar-mass stars on the low activity side of the Vaughan-Preston gap are the optimum observing targets for habitable Earth analogues.}}}}},
  langid = {english},
  file = {/Users/atomsite/Zotero/storage/XZYQN69U/See et al. - 2014 - The effects of stellar winds on the magnetospheres.pdf;/Users/atomsite/Zotero/storage/NISNKZXD/aa24323-14.html}
}

