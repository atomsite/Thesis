@article{blootRadioModellingBrightest2021,
  title = {Radio Modelling of the Brightest and Most Luminous Non-Thermal Colliding-Wind Binary {{Apep}}},
  author = {Bloot, S and Callingham, J R and Marcote, B},
  date = {2021-01-01},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
