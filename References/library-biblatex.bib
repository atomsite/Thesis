@article{dwekCoolingSputteringInfrared1996,
  title = {Cooling, {{Sputtering}}, and {{Infrared Emission}} from {{Dust Grains}} in {{Fast Nonradiative Shocks}}},
  author = {Dwek, Eli and Foster, Scott M. and Vancura, Olaf},
  date = {1996-01-01},
  journaltitle = {The Astrophysical Journal},
  volume = {457},
  pages = {244},
