@misc{dsilvaSpectroscopicMultiplicitySurvey2022,
  title = {A Spectroscopic Multiplicity Survey of {{Galactic Wolf-Rayet}} Stars. {{III}}. {{The}} Northern Late-Type Nitrogen-Rich Sample},
  author = {Dsilva, K. and Shenar, T. and Sana, H. and Marchant, P.},
  date = {2022-12-13},
  number = {arXiv:2212.06927},
  eprint = {2212.06927},
  eprinttype = {arxiv},
  primaryclass = {astro-ph},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/2212.06927},
  urldate = {2023-01-18},
  abstract = {Methods. We obtained high-resolution spectra of the complete magnitude-limited sample of 11 Galactic WNL stars with the Mercator telescope on the island of La Palma. We used cross-correlation with a log-likelihood framework to measure relative RVs and flagged binary candidates based on the peak-to-peak RV dispersion. By using Monte Carlo sampling and a Bayesian framework, we computed the three-dimensional likelihood and one-dimensional posteriors for the upper period cut-off (log PWmaNxL), power-law index ({$\pi$}WNL), and intrinsic binary fraction ( fiWntNL). Results. Adopting a threshold C of 50 km s-1, we derived foWbsNL = 0.36 {$\pm$} 0.15. Our Bayesian analysis produces fiWntNL = 0.42+-00..1157, {$\pi$}WNL = -0.70+-01..7032 and log PWmaNxL = 4.90+-03..0490 for the parent WNL population. The combined analysis of the Galactic WN population results in fiWntN = 0.52+-00..1142, {$\pi$}WN = -0.99+-00..5570 and log PWmaNx = 4.99+-01..0101. The observed period distribution of Galactic WN and WC binaries from the literature is in agreement with what is found. Conclusions. The period distribution of Galactic WN binaries peaks at P{$\sim$}1-10 d and that of the WC population at P{$\sim$}5000 d. This shift cannot be reconciled by orbital evolution due to mass loss or mass transfer. At long periods, the evolutionary sequence O (\textrightarrow - LBV)\textrightarrow - WN\textrightarrow - WC seems feasible. The high frequency of short-period WN binaries compared to WC binaries suggests that they either tend to merge, or that the WN components in these binaries rarely evolve into WC stars in the Galaxy.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  file = {/Users/atomsite/Zotero/storage/J9YAN223/Dsilva et al. - 2022 - A spectroscopic multiplicity survey of Galactic Wo.pdf}
}

@article{dsilvaSpectroscopicMultiplicitySurvey2022a,
  title = {A Spectroscopic Multiplicity Survey of {{Galactic Wolf-Rayet}} Stars: {{II}}. {{The}} Northern {{WNE}} Sequence},
  shorttitle = {A Spectroscopic Multiplicity Survey of {{Galactic Wolf-Rayet}} Stars},
  author = {Dsilva, K. and Shenar, T. and Sana, H. and Marchant, P.},
  date = {2022-08},
  journaltitle = {A\&A},
  volume = {664},
  pages = {A93},
  issn = {0004-6361, 1432-0746},
  doi = {10.1051/0004-6361/202142729},
  url = {https://www.aanda.org/10.1051/0004-6361/202142729},
  urldate = {2023-01-18},
  abstract = {Methods. We obtained high-resolution spectroscopic time series of the complete magnitude-limited sample of 16 WNE stars observable with the 1.2 m Mercator telescope at La Palma, typically providing a time base of about two to eight years. We measured relative radial velocities (RVs) using cross-correlation and used RV variations to flag binary candidates. Using an updated Monte Carlo method with a Bayesian framework, we calculated the three-dimensional likelihood for the intrinsic binary fraction ( fiWntNE), the maximum period (log Pmax), and the power-law index for the period distribution ({$\pi$}) for the WNE population with Pmin fixed at 1 d. We also used this updated method to re-derive multiplicity parameters for the Galactic WC population. Results. Adopting a peak-to-peak RV variability threshold of 50 km s-1 as a criterion, we classify 7 of the 16 targets as binaries. This results in an observed multiplicity fraction ( foWbsNE) of 0.44 {$\pm$} 0.12. Assuming flat priors, we derive the best-fit multiplicity properties fiWntNE = 0.56+-00..2105, log Pmax = 4.60+-00..4707, and {$\pi$} = -0.30+-00..5553 for the parent WNE population. We explored different mass-ratio distributions and note that they did not change our results significantly. For the Galactic WC population from Paper I, we re-derive fiWntC = 0.96+-00..0242, log Pmin = 0.75+-00..2660, log Pmax = 4.00+-00..4324, and {$\pi$} = 1.90-+11..2256. Conclusions. The derived multiplicity parameters for the WNE population are quite similar to those derived for main-sequence O binaries but differ from those of the WC population. The significant shift in the WC period distribution towards longer periods is too large to be explained via expansion of the orbit due to stellar winds, and we discuss possible implications of our results. Analysis of the WNL population and further investigation of various evolutionary scenarios is required to connect the different evolutionary phases of stars at the upper-mass end.},
  langid = {english},
  file = {/Users/atomsite/Zotero/storage/TC5ZB6D6/Dsilva et al. - 2022 - A spectroscopic multiplicity survey of Galactic Wo.pdf}
}

@article{ducheneStellarMultiplicity2013,
