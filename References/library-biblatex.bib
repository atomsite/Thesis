@article{pulsMassLossHot2008,
  title = {Mass Loss from Hot Massive Stars},
  author = {Puls, Joachim and Vink, Jorick S. and Najarro, Francisco},
  date = {2008-12-01},
  journaltitle = {Astron Astrophys Rev},
  volume = {16},
  number = {3},
