@article{monteuxWaterInteriorStructure2018,
  title = {Water and the {{Interior Structure}} of {{Terrestrial Planets}} and {{Icy Bodies}}},
  author = {Monteux, J. and Golabek, G. J. and Rubie, D. C. and Tobie, G. and Young, E. D.},
