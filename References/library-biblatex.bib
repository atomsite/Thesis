@article{castor_radiation-driven_1975,
  title = {Radiation-Driven Winds in {{Of}} Stars},
  author = {Castor, J. I. and Abbott, D. C. and Klein, R. I.},
  date = {1975-01},
  journaltitle = {The Astrophysical Journal},
  volume = {195},
  pages = {157},
  issn = {0004-637X, 1538-4357},
  doi = {10.1086/153315},
  url = {http://adsabs.harvard.edu/doi/10.1086/153315},
  urldate = {2018-08-05},
  abstract = {The large number of subordinate lines of a representative ion are found to have a dominant effect on the force of radiation on material in O star atmospheres. The force is increased over that due to resonance lines alone so that rates of mass loss are obtained which are 100 times greater than previously thought possible. The force is related to the solution of the line-transfer problem, and it becomes a function of the local velocity gradient. A new stellar wind theory, with a different interpretation of the singular point, is developed to treat this situation. The rate of mass loss, and other properties of the model, are uniquely specified by the luminosity, mass, and radius of the star. Alternative static models do not exist. Numerical results give a rate of mass loss 6 x 10``6 221\textcopyright{} per year for an 05 star, with a terminal velocity of 1500 km s``1. The rate of mass loss is sensitive to stellar parameters, while the terminal velocity is not. The continuum optical depth in the expanding envelope is about 0.16, of the right order to explain the reduced brightness temperature observed in \textsterling{} Pup. There is sufficient mass in the envelope for recombination to produce the emission lines of H and of He n which are observed, with approximately the proper strength. The rate of mass loss corresponds to a loss of more than 25 percent of the star's mass during main-sequence hydrogen burning, with obvious consequences for stellar evolution, and with the possibility of modified surface abundances.},
  langid = {english},
  file = {/Users/py13je/Zotero/storage/Q8CJ9872/Castor et al. - 1975 - Radiation-driven winds in Of stars.pdf}
}

