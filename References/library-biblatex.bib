@article{linNumericalModelingProtocore2009,
  title = {Numerical Modeling of Protocore Destabilization during Planetary Accretion: {{Methodology}} and Results},
  shorttitle = {Numerical Modeling of Protocore Destabilization during Planetary Accretion},
  author = {Lin, Ja-Ren and Gerya, Taras V. and Tackley, Paul J. and Yuen, David A. and Golabek, Gregor J.},
  date = {2009-12-01},
  journaltitle = {Icarus},
  volume = {204},
