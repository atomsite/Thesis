@article{sanderGalacticWCStars2012a,
  title = {The {{Galactic WC}} Stars. {{Stellar}} Parameters from Spectral Analyses Indicate a New Evolutionary Sequence},
  author = {Sander, A. and Hamann, W.-R. and Todt, H.},
  date = {2012-04},
  journaltitle = {Astronomy \&amp; Astrophysics, Volume 540, id.A144, {$<$}NUMPAGES{$>$}79{$<$}/NUMPAGES{$>$} pp.},
