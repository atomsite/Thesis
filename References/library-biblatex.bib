@article{parkerDidSolarSystem2016,
  title = {Did the {{Solar}} System Form in a Sequential Triggered Star Formation Event?},
  author = {Parker, Richard J. and Dale, James E.},
  date = {2016-02-01},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {456},
  pages = {1066--1072},
  issn = {0035-8711},
  doi = {10.1093/mnras/stv2765},
  url = {https://ui.adsabs.harvard.edu/abs/2016MNRAS.456.1066P},
  urldate = {2022-05-02},
  abstract = {The presence and abundance of the short-lived radioisotopes (SLRs) 26Al and 60Fe during the formation of the Solar system is difficult to explain unless the Sun formed in the vicinity of one or more massive star(s) that exploded as supernovae. Two different scenarios have been proposed to explain the delivery of SLRs to the protosolar nebula: (I) direct pollution of the protosolar disc by supernova ejecta, and (II) the formation of the Sun in a sequential star formation event in which supernovae shockwaves trigger further star formation which is enriched in SLRs. The sequentially triggered model has been suggested as being more astrophysically likely than the direct pollution scenario. In this paper, we investigate this claim by analysing a combination of N-body and smoothed particle hydrodynamics simulations of star formation. We find that sequential star formation would result in large age spreads (or even bi-modal age distributions for spatially coincident events) due to the dynamical relaxation of the first star formation event(s). Secondly, we discuss the probability of triggering spatially and temporally discrete populations of stars and find this to be only possible in very contrived situations. Taken together, these results suggest that the formation of the Solar system in a triggered star formation event is as improbable, if not more so, than the direct pollution of the protosolar disc by a supernova.},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Earth and Planetary Astrophysics,Astrophysics - Solar and Stellar Astrophysics,H II regions,methods: numerical,open clusters and associations: general,Physics - Geophysics,planets and satellites: formation,stars: formation},
  annotation = {ADS Bibcode: 2016MNRAS.456.1066P},
  file = {/Users/atomsite/Zotero/storage/PPFIVUFA/Parker and Dale - 2016 - Did the Solar system form in a sequential triggere.pdf}
}

@article{parkerSupernovaEnrichmentDynamical2014,
  title = {Supernova Enrichment and Dynamical Histories of Solar-Type Stars in Clusters},
  author = {Parker, Richard J. and Church, Ross P. and Davies, Melvyn B. and Meyer, Michael R.},
  date = {2014-01-01},
