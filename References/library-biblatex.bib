@article{gormaz-matamalaNewHydrodynamicSolutions2021,
  title = {New {{Hydrodynamic Solutions}} for {{Line-driven Winds}} of {{Hot Massive Stars Using}} the {{Lambert W-function}}},
  author = {Gormaz-Matamala, A. C. and Cur\'e, M. and Hillier, D. J. and Najarro, F. and Kub\'atov\'a, B. and Kub\'at, J.},
  date = {2021-10-01},
